import System.Environment
import Data.List

loadLines :: String -> IO [String]
loadLines filePath = do
  fileCont <- readFile filePath
  return $ lines fileCont

loadRun :: String -> IO ()
loadRun filePath = do
  let fileLines = loadLines filePath
  let result = "Result"
  putStrLn result
  
main :: IO ()
main = do
  args <- getArgs 
  loadRun (head args)
